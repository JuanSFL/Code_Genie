ts] },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n\n\n    },\n  });\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addThought({\n        variables: {\n          thoughtText,\n          thoughtAuthor: Auth.getProfile().data.username,\n        },\n      });\n\n      setThoughtText('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === 'thoughtText' && value.length <= 280) {\n      setThoughtText(value);\n      setCharacterCount(value.length);\n    }\n  };\n\n  return (\n    <div>\n      <h3 className=\"type-white\">What's on your techy mind?</h3>\n\n      {Auth.loggedIn() ? (\n        <>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"question-ask\">\n              <textarea\n                name=\"thoughtText\"\n                placeholder=\"Ask your question here\"\n                value={thoughtText}\n                className=\"ask-input\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <p\n            className={`type-white ${\n              characterCount === 280 || error ? 'text-danger' : ''\n            }`}\n          >\n            Character Count: {characterCount}/280\n          </p>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"flashy-btn\" type=\"submit\">\n                Add Thought\n              </button>\n            </div>\n            {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to share your thoughts. Please{' '}\n          <Link to=\"/login\">Login</Link> or <Link to=\"/signup\">Signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default Ask;\n","/Users/jaileneveras/Documents/proj3/client/src/components/SearchBar/index.js",[],{"ruleId":"111","severity":1,"message":"112","line":10,"column":26,"nodeType":"113","messageId":"114","endLine":10,"endColumn":30},{"ruleId":"111","severity":1,"message":"115","line":8,"column":9,"nodeType":"113","messageId":"114","endLine":8,"endColumn":24},{"ruleId":"111","severity":1,"message":"116","line":12,"column":9,"nodeType":"113","messageId":"114","endLine":12,"endColumn":25},{"ruleId":"111","severity":1,"message":"117","line":2,"column":10,"nodeType":"113","messageId":"114","endLine":2,"endColumn":14},{"ruleId":"111","severity":1,"message":"118","line":15,"column":21,"nodeType":"113","messageId":"114","endLine":15,"endColumn":26},{"ruleId":"111","severity":1,"message":"112","line":15,"column":28,"nodeType":"113","messageId":"114","endLine":15,"endColumn":32},{"ruleId":"119","severity":1,"message":"120","line":24,"column":9,"nodeType":"121","endLine":24,"endColumn":49},{"ruleId":"122","severity":1,"message":"123","line":34,"column":18,"nodeType":"121","endLine":34,"endColumn":87},{"ruleId":"119","severity":1,"message":"124","line":44,"column":9,"nodeType":"121","endLine":44,"endColumn":50},{"ruleId":"125","severity":1,"message":"126","line":41,"column":1,"nodeType":"127","endLine":41,"endColumn":34},{"ruleId":"111","severity":1,"message":"128","line":6,"column":26,"nodeType":"113","messageId":"114","endLine":6,"endColumn":34},{"ruleId":"111","severity":1,"message":"112","line":41,"column":15,"nodeType":"113","messageId":"114","endLine":41,"endColumn":19},{"ruleId":"111","severity":1,"message":"112","line":19,"column":15,"nodeType":"113","messageId":"114","endLine":19,"endColumn":19},{"ruleId":"122","severity":1,"message":"123","line":13,"column":5,"n