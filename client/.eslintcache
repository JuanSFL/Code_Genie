nt}/280\n            {error && <span className=\"ml-2\">{error.message}</span>}\n          </p>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"flashy-btn under\" type=\"submit\">\n                Add Comment\n              </button>\n            </div>\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to share your thoughts. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default CommentForm;\n",{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":2,"message":"102","line":95,"column":27,"nodeType":"103","messageId":"104","endLine":95,"endColumn":34},{"ruleId":"105","severity":1,"message":"106","line":10,"column":26,"nodeType":"107","messageId":"108","endLine":10,"endColumn":30},{"ruleId":"109","severity":1,"message":"110","line":33,"column":7,"nodeType":"111","endLine":33,"endColumn":45},{"ruleId":"105","severity":1,"message":"112","line":2,"column":10,"nodeType":"107","messageId":"108","endLine":2,"endColumn":14},{"ruleId":"105","severity":1,"message":"113","line":15,"column":21,"nodeType":"107","messageId":"108","endLine":15,"endColumn":26},{"ruleId":"105","severity":1,"message":"106","line":15,"column":28,"nodeType":"107","messageId":"108","endLine":15,"endColumn":32},{"ruleId":"105","severity":1,"message":"114","line":6,"column":26,"nodeType":"107","messageId":"108","endLine":6,"endColumn":34},{"ruleId":"105","severity":1,"message":"106","line":36,"column":15,"nodeType":"107","messageId":"108","endLine":36,"endColumn":19},{"ruleId":"115","severity":1,"message":"116","line":24,"column":9,"nodeType":"111","endLine":24,"endColumn":49},{"ruleId":"109","severity":1,"message":"110","line":34,"column":18,"nodeType":"111","endLine":34,"endColumn":87},{"ruleId":"115","severity":1,"message":"117","line":44,"column":9,"nodeType":"111","endLine":44,